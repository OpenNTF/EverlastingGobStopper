Function WebReturnErrorAlert (strErrorMsg As String, strURL As String) As String
	'
	'Returns an alert message and loads a URL - if the URL is blank, a back function is returned
	'
	Const strHTML1 = "<script language=""JavaScript""> alert("""
	Const strHTML2 = """); "
	Const strHTML3 = "history.back(); "
	Const strHTML4 = "window.location = """
	Const strHTML5 = """; "
	Const strHTML6 = "</script>"
	
	If strURL = "" Then
		WebReturnErrorAlert = strHTML1 & strErrorMsg & strHTML2 & strHTML3 & strHTML6
	Else
		WebReturnErrorAlert = strHTML1 & strErrorMsg & strHTML2 & strHTML4 & strURL & strHTML5 & strHTML6
	End If
End Function
'++LotusScript Development Environment:2:1:WebReturnParent:1:8
Function WebReturnParent (strURL As String)
	'
	'Loads a URL in the parent window of the current window
	'
	Const strHTML1 = "<script language=""JavaScript""> window.parent.location = """
	Const strHTML2 = """; </script>"
	
	WebReturnParent = strHTML1 & strURL & strHTML2
End Function
